{
    "name": "jordisala1991/archetype-sonata",
    "description": "Sonata archetype",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Jordi Sala Morales",
            "email": "jordism91@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",

        "doctrine/doctrine-bundle": "1.6.7",
        "doctrine/orm": "2.5.6",
        "incenteev/composer-parameter-handler": "2.1.2",
        "sensio/distribution-bundle": "5.0.18",
        "symfony/monolog-bundle": "3.0.3",
        "symfony/swiftmailer-bundle": "2.5.4",
        "symfony/symfony": "2.8.18",

        "doctrine/doctrine-migrations-bundle": "1.2.1",
        "egeloen/ckeditor-bundle": "5.0.1",
        "friendsofsymfony/user-bundle": "1.3.7",
        "sonata-project/admin-bundle": "3.14.0",
        "sonata-project/block-bundle": "3.3.2",
        "sonata-project/classification-bundle": "3.3.0",
        "sonata-project/core-bundle": "3.3.0",
        "sonata-project/doctrine-orm-admin-bundle": "3.1.4",
        "sonata-project/easy-extends-bundle": "2.1.10",
        "sonata-project/formatter-bundle": "3.2.1",
        "sonata-project/intl-bundle": "2.3.0",
        "sonata-project/media-bundle": "3.5.0",
        "sonata-project/user-bundle": "3.2.3",
        "twig/twig": "2.3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.1.2",
        "phpmd/phpmd": "2.6.0",
        "phpunit/phpunit": "5.7.17",
        "sensio/generator-bundle": "3.1.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
