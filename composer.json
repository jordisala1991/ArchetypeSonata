{
    "name": "jordisala1991/archetype-sonata",
    "type": "project",
    "description": "This is my Sonata project template",
    "homepage": "https://github.com/jordisala1991/ArchetypeSonata",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordi Sala Morales",
            "email": "jordism91@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "doctrine/doctrine-bundle": "2.0.7",
        "doctrine/doctrine-migrations-bundle": "2.1.2",
        "doctrine/orm": "2.7.3",
        "friendsofsymfony/ckeditor-bundle": "1.2.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "sonata-project/admin-bundle": "3.67.0",
        "sonata-project/block-bundle": "3.18.5",
        "sonata-project/core-bundle": "3.18.0",
        "sonata-project/doctrine-orm-admin-bundle": "3.17.1",
        "sonata-project/easy-extends-bundle": "2.5.0",
        "sonata-project/formatter-bundle": "4.1.3",
        "sonata-project/media-bundle": "3.24.0",
        "sonata-project/user-bundle": "4.5.3",
        "symfony/dotenv": "5.0.6",
        "symfony/flex": "1.7.1",
        "symfony/framework-bundle": "4.4.6",
        "symfony/monolog-bundle": "3.5.0",
        "symfony/security-bundle": "4.4.6",
        "symfony/swiftmailer-bundle": "3.4.0",
        "symfony/twig-bundle": "4.4.8",
        "symfony/yaml": "5.0.6",
        "twig/twig": "2.12.5"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "2.3.2",
        "friendsofphp/php-cs-fixer": "2.16.1",
        "phpstan/phpstan": "0.12.25",
        "phpstan/phpstan-doctrine": "0.12.13",
        "phpstan/phpstan-symfony": "0.12.4",
        "symfony/debug-bundle": "5.0.6",
        "symfony/web-profiler-bundle": "5.0.6"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4 || ^5.0"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "assets:install %PUBLIC_DIR% --symlink": "symfony-cmd"
        },
        "lint-twig": "bin/console lint:twig templates",
        "lint-yaml": "bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "php-cs-fixer fix --ansi --verbose",
        "phpstan": "phpstan analyse --no-progress"
    }
}
