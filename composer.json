{
    "name": "jordisala1991/archetype-sonata",
    "description": "Sonata archetype",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Jordi Sala Morales",
            "email": "jordism91@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",

        "doctrine/doctrine-bundle": "1.8.1",
        "doctrine/orm": "2.6.0",
        "incenteev/composer-parameter-handler": "2.1.3",
        "sensio/distribution-bundle": "5.0.21",
        "symfony/config": "3.4.4",
        "symfony/console": "3.4.4",
        "symfony/debug": "3.4.4",
        "symfony/debug-bundle": "3.4.4",
        "symfony/dependency-injection": "3.4.4",
        "symfony/framework-bundle": "3.4.4",
        "symfony/http-foundation": "3.4.4",
        "symfony/http-kernel": "3.4.4",
        "symfony/monolog-bundle": "3.1.2",
        "symfony/security-bundle": "3.4.4",
        "symfony/swiftmailer-bundle": "3.2.0",
        "symfony/twig-bundle": "3.4.4",
        "symfony/validator": "3.4.4",
        "symfony/web-profiler-bundle": "3.4.4",

        "doctrine/doctrine-migrations-bundle": "1.3.1",
        "friendsofsymfony/user-bundle": "2.1.1",
        "egeloen/ckeditor-bundle": "6.0.1 as 5.0.3",
        "sonata-project/admin-bundle": "3.31.1",
        "sonata-project/block-bundle": "3.12.0",
        "sonata-project/core-bundle": "3.9.1",
        "sonata-project/doctrine-orm-admin-bundle": "3.4.2",
        "sonata-project/easy-extends-bundle": "2.5.0",
        "sonata-project/formatter-bundle": "3.4.1",
        "sonata-project/media-bundle": "3.11.0",
        "sonata-project/user-bundle": "4.1.1",
        "twig/twig": "2.4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.10.3",
        "sensio/generator-bundle": "3.1.7"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
